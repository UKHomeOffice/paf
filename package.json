{
  "name": "paf",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/paf.git"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "hof-build watch",
    "dev": "hof-build watch --env",
    "test": "yarn run test:lint && yarn run test:unit && yarn run test:ui-integration",
    "test:lint": "eslint .",
    "test:ui-integration": "NODE_ENV=test _mocha \"test/_ui-integration/**/*.spec.js\" --exit",
    "test:unit": "NODE_ENV=test nyc _mocha \"test/_unit/**/*.spec.js\"",
    "build": "hof-build",
    "postinstall": "yarn run build"
  },
  "dependencies": {
    "accessible-autocomplete": "^2.0.2",
    "busboy-body-parser": "^0.3.2",
    "device": "^0.3.12",
    "hof": "^20.1.2",
    "jimp": "^0.16.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "typeahead-aria": "^1.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "7.30.0",
    "eslint-config-hof": "^1.3.1",
    "jsdom": "^16.4.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "supertest": "^5.0.0",
    "supertest-session": "^4.1.0"
