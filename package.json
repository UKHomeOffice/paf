{
  "name": "paf",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=20.19.0 <21.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/paf.git"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "[ -f .env ] && hof-build watch --env || hof-build watch",
    "test": "NODE_ENV=test yarn run test:lint && yarn run test:unit && yarn run test:ui-integration",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:ui-integration": "NODE_ENV=test _mocha \"test/_ui-integration/**/*.spec.js\" --exit",
    "test:unit": "NODE_ENV=test nyc _mocha \"test/_unit/**/*.spec.js\"",
    "build": "hof-build",
    "test:snyk": "snyk config set api=SNYK_TOKEN && snyk test",
    "postinstall": "yarn run build"
  },
  "dependencies": {
    "accessible-autocomplete": "^3.0.1",
    "bl": "^6.1.3",
    "busboy": "^1.6.0",
    "busboy-body-parser": "^0.3.2",
    "form-data": "^4.0.4",
    "hof": "^22.10.4",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "sqs-producer": "^3.4.0",
    "typeahead-aria": "^1.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "chromedriver": "^118.0.1",
    "eslint": "^8.57.0",
    "eslint-config-hof": "^1.3.4",
    "jquery": "^3.3.1",
    "jsdom": "^16.4.0",
    "mocha": "^9.2.1",
    "mock-fs": "^5.0.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "snyk": "^1.1107.0",
    "supertest": "^5.0.0",
    "supertest-session": "^4.1.0"
  },
  "mocha": {
    "reporter": "spec",
    "require": "test/setup.js",
    "recursive": "true",
    "timeout": "9000"
  },
  "keywords": [],
  "author": ""
}
